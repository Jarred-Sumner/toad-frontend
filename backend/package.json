    "eslint-config-import": "^0.13.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.8.0",
    "lint-staged": "^5",
    "nodemon": "^1.17.5",
    "prettier": "^1.5.2"
  },
  "dependencies": {
    "apollo-link-schema": "^1.1.0",
    "apollo-server-express": "^1.3.6",
    "body-parser": "^1.18.3",
    "deo": "^0.0.2",
    "express": "^4.16.3",
    "graphql": "^0.13.2",
    "graphql-depth-limit": "^1.1.0",
    "graphql-disable-introspection": "^1.0.1",
